@{
    Layout = null;
}

<!DOCTYPE html>

<html>


<head>
    <title> Mira-Maré Praia Hotel  - Praia da Enseada - Guarujá - São Paulo - SP -  @ViewBag.Title</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    
    <meta property="og:title" content="Mira-Maré Praia Hotel" />
    <meta name="description" content="Hotel na Praia da Enseada, Guarujá - São Paulo">
    <meta name="og:description" content="Hotel de frente para o mar na Praia da Enseada, Guarujá - São Paulo, dista poucas quadras dos melhores restaurantes do Guarujá, de um centro de esportes náuticos, de um centro comercial e da estrada de acesso à Praia de Pernambuco e demais praias do Litoral Norte (Costa Verde).">


    <!-- Styles -->
    <link href="@Url.Content("~/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/bootstrap-responsive.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/css/bootstrap-overrides.css")" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/theme.css")" />
    <link rel="stylesheet" href="@Url.Content("~/css/index.css")" type="text/css" media="screen" />
    <link href='http://fonts.googleapis.com/css?family=Lato:300,400,700,900,300italic,400italic,700italic,900italic' rel='stylesheet' type='text/css' />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/lib/animate.css")" media="screen, projection" />
    
    <link rel="icon" href="@Url.Content("~/Img/favicon.ico")" type="image/x-icon" />



    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<body class="pull_top">

    <div class="navbar transparent navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="~/">
                    <strong>Mira-Maré Praia Hotel</strong>
                </a>
                <div class="nav-collapse collapse">
                    @Html.Partial("_Menu")
                </div>
            </div>
        </div>
    </div>

    @RenderBody()
    
    
    
    
    

    <!-- starts footer -->
  @Html.Partial("_Footer")


    <!-- Scripts -->

    <script src="@Url.Content("~/js/jquery-latest.js")"></script>
    <script src="@Url.Content("~/js/bootstrap.js")"></script>
    <script src="@Url.Content("~/js/theme.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/js/index-slider.js")"></script>
    <script src="~/js/Portugues/portugues.js"></script>




</body>



</html>

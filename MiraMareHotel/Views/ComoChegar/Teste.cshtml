
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Teste</title>
</head>
    <body>
      
        <div id="map" class="gmap"></div>
        
        <iframe width="200" height="200" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com.mx/?ie=UTF8&amp;ll=64.089157,-21.816616&amp;spn=0.045157,0.15398&amp;t=m&amp;z=13&amp;output=embed"></iframe>
     
        
        <!-- Java Scripts -->
        <script src="@Url.Content("~/javascripts/jquery-1.8.3.min.js")"></script>
        <!-- Google Map Javascripts -->
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
        <script type="text/javascript" src="@Url.Content("~/javascripts/gmap3.min.js")"></script>
        <script type="text/javascript">
            $(function () {
                $('#map').gmap3({
                    marker: {
                        address: "Ohe Street Honolulu, Hawaii 96813"
                    },
                    map: {
                        options: {
                            zoom: 14
                        }
                    }
                });
            });
        </script>
        <!-- End Google Map Javascripts -->



        <script src="@Url.Content("~/javascripts/jquery.gmap-1.1.0-min.html")"></script>

        

    </body>




</html>
